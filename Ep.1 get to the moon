const fristName: string = 'Atipat' // const ทำการประกาศ
    let y: number = 10 // การใช้เลขในตัวแปรy
                y = 20 // เมื่อ y = 20
                
let isWantToGoHome: boolean  = false // เป็นการเช็ค ture or false
console.log(fristName) //ประกาศไฟล์ fristName
console.log(y) // ประกาศตัวแปร Y
console.log(isWantToGoHome) //ประกาศ boolean จากคำถาม
// 2
const a: number = 9 // ใส่ตัวแปร a โดยไม่เปลี่ยนเลข
const b: number = 9 // ใส่ตัวแปร b โดยไม่เปลี่ยนเลข

console.log(a === b) // ประกาศผลลัพธ์ (a === b)
console.log(a > b) // ประกาศผลลัพธ์ (a > b)
console.log(a <= b) // ประกาศผลลัพธ์ (a <= b)